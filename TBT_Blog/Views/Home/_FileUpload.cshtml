@model List<TBT_Blog.Models.Image>
<div class="form-inline">
   


    @if (Model.Count != 0)
    {
        foreach (var img in Model)
        {
            <div>
                <a href="@img.ImageUrl">@img.Name</a>
                <div class="icon-bar">
                    <img src="@img.ImageUrl" alt="@img.Name" style="width:80px;height:80px" />
                </div>

            </div>
        }
    }





</div>